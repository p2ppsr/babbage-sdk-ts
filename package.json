    "@babbage/sdk": "^0.4.41",
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@types/jest": "^29.4.0",
    "@types/node": "^20.8.2",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "babel-loader": "^8.2.5",
    "cwi-base": "^0.3.107",
    "jest": "^29.4.3",
    "pushdrop": "^0.1.32",
