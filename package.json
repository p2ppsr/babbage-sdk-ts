{
  "name": "@babbage/sdk-ts",
  "version": "0.2.51",
  "description": "Build Babbage Apps with TypeScript",
  "main": "./out/src/index.js",
  "types": "./out/src/index.d.ts",
  "repository": {
    "type": "git",
